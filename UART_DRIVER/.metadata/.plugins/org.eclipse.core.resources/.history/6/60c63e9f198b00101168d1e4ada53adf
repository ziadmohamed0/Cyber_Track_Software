#include "../../Drivers/custom_driver/hal/hal_defs.h"

int main (int argc, char* argv[]) {
	SystemInit();
    ultrasonic ultra_front(GPIO_PORT::A, 0, GPIO_PORT::A, 1, TIM2);
    ultra_front.init();
	while(true) {
        ultra_front.trigger();
        delay::ms(100);

	}
	return 0;
}
